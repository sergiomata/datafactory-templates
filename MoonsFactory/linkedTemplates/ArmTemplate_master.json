{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "MoonsFactory"
		},
		"Moons_Production_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Moons_Production'"
		},
		"datalakemoons_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'datalakemoons'"
		},
		"Moons_Production_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "e3ccd3611122480aa468dd05281abdea"
		},
		"datalakemoons_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://moonsdatalake.dfs.core.windows.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Moons_Production_connectionString": {
						"value": "[parameters('Moons_Production_connectionString')]"
					},
					"datalakemoons_accountKey": {
						"value": "[parameters('datalakemoons_accountKey')]"
					},
					"Moons_Production_properties_typeProperties_database": {
						"value": "[parameters('Moons_Production_properties_typeProperties_database')]"
					},
					"datalakemoons_properties_typeProperties_url": {
						"value": "[parameters('datalakemoons_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}